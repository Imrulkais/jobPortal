{{> header}}

<div class="row">
{{#jobs}}
<table class="table borderless">
  <tbody>
	  <tr>
	      <td>Company Name:</td>
	      <td>{{companyname}}</td>
	  </tr>
	  <tr>
	      <td>Job title:</td>
	      <td><a href="/singleJobDetails?id={{id}}">{{jobtitle}}</td>
	  </tr>
	  <tr>
	      <td>Number of vacancies:</td>
	      <td>{{noofvacancies}}</td>
	  </tr>
	  <tr>
	      <td>Deadline:</td>
	      <td>{{applicationdeadline}}</td>
	  </tr>
  </tbody>
</table>
{{/jobs}}
</div>

{{> footer}}